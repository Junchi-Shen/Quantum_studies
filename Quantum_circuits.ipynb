   "source": [
    "# Basic Quantum circuit with PennyLane\n",
    "\n",
    "import pennylane as qml\n",
    "\n",
    "def my_quantum_function(x, y):\n",
    "    qml.RZ(x, wires=0)\n",
    "    qml.CNOT(wires=[0,1])\n",
    "    qml.RY(y, wires=1)\n",
    "    return qml.expval(qml.Z(wires=1))"
   ]
  }
